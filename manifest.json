{
    "manifest_version": 2,
    "name": "Dark Theme for Google Searches",
    "short_name": "Google Dark Theme",
    "description": "A nice dark theme for google searches",
    "version": "13.103",
    "homepage_url": "https://elijahlopez.herokuapp.com/software/#google-dark-theme",
